<Window x:Class="Hra.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hra"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <Frame NavigationUIVisibility="Hidden" Name="MainFrame" HorizontalAlignment="Center" Width="800" Height="450" VerticalAlignment="Center"/>
        <Frame NavigationUIVisibility="Hidden" Name="InvFrame"/>
        <Rectangle Fill="#FFFFFF" HorizontalAlignment="Right" Height="103" Stroke="Black" VerticalAlignment="Top" Width="113"/>
        <Button Click="OpenCloseInv" HorizontalAlignment="Right" Height="103" VerticalAlignment="Top" Width="113" Background="Transparent" BorderBrush="Transparent"/>
    </Grid>
</Window>
